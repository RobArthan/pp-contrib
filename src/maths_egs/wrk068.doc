val eq_thms = map (all_undisch_rule o all_µ_elim o rewrite_rule[]) all_thms drop (not o is_eq o concl);
val ç_thms = map §_t_intro (map (all_undisch_rule o all_µ_elim o rewrite_rule[])
	all_thms drop (is_eq o concl));
=TEX
